{
  "name": "ef-language-service-vscode",
  "private": true,
  "description": "VSCode extension for efml language server",
  "author": "Jack Works",
  "license": "MIT",
  "version": "0.0.0",
  "main": "./dist/extension.js",
  "enableProposedApi": false,
  "publisher": "MagicWorks",
  "activationEvents": [
    "onLanguage:efml"
  ],
  "contributes": {
    "semanticTokenScopes": [
      {
        "language": "efml",
        "scopes": {
          "attribute": [
            "meta.line.attr.efml",
            "entity.other.attribute-name.html"
          ],
          "property": [
            "meta.line.prop.efml"
          ],
          "templateExpression": [
            "expression.embbeded.efml"
          ],
          "mountingPoint": [
            "storage.type.eft"
          ],
          "listMountingPoint": [
            "storage.type.eft"
          ],
          "escape": [
            "constant.character.escape.efml"
          ],
          "element": [
            "entity.name.tag.block.any.efml"
          ],
          "elementStart": [
            "punctuation.definition.tag.begin.efml"
          ],
          "class": [
            "entity.other.attribute-name.class.css"
          ],
          "reference": [
            "entity.other.attribute-name.id.html"
          ],
          "mustacheStart": [
            "punctuation.definition.template-expression.begin"
          ],
          "mustacheEnd": [
            "punctuation.definition.template-expression.end"
          ],
          "exoticExpression": [
            "variable"
          ]
        }
      }
    ],
    "languages": [
      {
        "id": "efml",
        "extensions": [
          ".ef",
          ".efml"
        ],
        "aliases": [
          "ef.js",
          "ef"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jack-Works/ef-language-service"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "dependencies": {
    "ef-language-service-server": "*",
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.52.0",
    "del": "^6.0.0",
    "rollup": "^2.41.2",
    "vsce": "^1.87.0",
    "vscode-test": "^1.5.1"
  },
  "scripts": {
    "build": "rollup -c"
  }
}
